{
  "env": {
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "plugin:unicorn/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-use-before-define": "error",
    "no-template-curly-in-string": "error",
    "unicorn/prefer-module": "off",
    "unicorn/prevent-abbreviations": "off"
  },
  "plugins": ["jest", "unicorn"]
}
